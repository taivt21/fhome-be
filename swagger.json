{
    "openapi": "3.0.1",
    "info": {
      "title": "API Documentation",
      "description": "Documentation for the API endpoints",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:3000",
        "description": "Local development server"
      }
    ],
    "paths": {
      "/api/users": {
        "get": {
          "summary": "Get all users",
          "operationId": "getAllUsers",
          "responses": {
            "200": {
              "description": "Success",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Users"
                  }
                }
              }
            },
            "500": {
              "description": "Internal server error"
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "User": {
          "type": "object",
          "properties": {
            "email": {
              "type": "string"
            },
            "password": {
              "type": "string"
            },
            "phoneNumber": {
              "type": "string"
            },
            "fullname": {
              "type": "string"
            },
            "img": {
              "type": "string"
            },
            "status": {
              "type": "string"
            },
            "roleName": {
              "type": "string"
            }
          }
        },
        "Users": {
          "type": "array",
          "items": {
            "$ref": "#/components/schemas/User"
          }
        }
      }
    }
  }
  